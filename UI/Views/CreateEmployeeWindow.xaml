<Window x:Class="UI.Views.CreateEmployeeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UI.Views"
        mc:Ignorable="d"
        Title="Tilføj Medarbejder" Height="224.361" Width="213.598" Background="#FF0C9368">
    <Grid Margin="0,0,2,1">
        <TextBox x:Name="FirstNameTB" Height="23" Margin="42,87,42,0" TextWrapping="Wrap" Text="Fornavn" VerticalAlignment="Top" Width="120" TextChanged="FirstNameTB_TextChanged"/>
        <TextBox x:Name="LastNameTB" HorizontalAlignment="Left" Height="23" Margin="42,115,0,0" TextWrapping="Wrap" Text="Efternavn" VerticalAlignment="Top" Width="120" TextChanged="LastNameTB_TextChanged"/>
        <Button x:Name="CreateEmployeeBtn" Content="Tilføj" HorizontalAlignment="Left" Margin="42,143,0,0" VerticalAlignment="Top" Width="120" Height="26" Background="#FF4AF762" Click="CreateEmployeeBtn_Click"/>
        <ComboBox x:Name="RankCB" HorizontalAlignment="Left" Margin="42,60,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="RankCB_SelectionChanged"/>
        <Label Content="Vælg stilling" HorizontalAlignment="Left" Margin="42,30,0,0" VerticalAlignment="Top" Width="120"/>
    </Grid>
</Window>
